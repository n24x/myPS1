set nocompatible
filetype plugin on
syntax on
set number
set relativenumber
set cursorline
set expandtab
set ignorecase
set nowrap
set history=1000
set wildmenu
" autocmd FileType yaml setlocal ai ts=2 sw=2 et " cuc nu
set autoindent
set tabstop=2
set shiftwidth=2
colorscheme desert
set encoding=utf-8
autocmd FileType yaml setlocal ai ts=2 sw=2 et cuc
autocmd FileType yml setlocal ai ts=2 sw=2 et cuc
